		<Upper>100</Upper>
	</FoodHealthRange>
  <CleanValueRange>
    <Lower>0</Lower>
    <Upper>100</Upper>
  </CleanValueRange>
	<CleanHealthRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</CleanHealthRange>
	<SickHealthRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</SickHealthRange>
  <SickValueRange>
    <Lower>0</Lower>
    <Upper>100</Upper>
  </SickValueRange>
	<FoodHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</FoodHappinessRange>
	<ClothesHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</ClothesHappinessRange>
	<HourseHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</HourseHappinessRange>
	<ClubHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</ClubHappinessRange>
	<ChurchHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</ChurchHappinessRange>
	<MournHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</MournHappinessRange>
	<DramaHappinessRange>
		<Lower>0</Lower>
		<Upper>100</Upper>
	</DramaHappinessRange>
  <!--市民寿命区间-->
  <DeathDaysRange>
    <Lower>4950</Lower>
    <Upper>8100</Upper>
  </DeathDaysRange>
  <!--市民生育年龄-->
  <PregnancyRange>
    <Lower>1260</Lower>
    <Upper>4950</Upper>    
  </PregnancyRange>
  <!--生育周期-->
  <PregnancyInterval>
    <Lower>300</Lower>
    <Upper>390</Upper>
  </PregnancyInterval>
  <!--健康度-->
  <HealthRange>
    <Lower>0</Lower>
    <Upper>2000</Upper>
  </HealthRange>
  <!--幸福度-->
  <HappyRange>
    <Lower>0</Lower>
    <Upper>2000</Upper>
  </HappyRange>
</CitizenAttributeRangeConfig>